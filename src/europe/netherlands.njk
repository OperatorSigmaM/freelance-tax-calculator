    <script>
function calculateCredits(grossIncome, isEmployee) {
    let generalCredit = 0;
    let labourCredit = 0;
    const incomeForCredits = grossIncome;
    
    const AHK_MAX = 3360;
    const AHK_LOW_THRESHOLD = 22000;
    const AHK_HIGH_THRESHOLD = 76000;
    
    if (incomeForCredits <= AHK_LOW_THRESHOLD) {
        generalCredit = AHK_MAX;
    } else if (incomeForCredits < AHK_HIGH_THRESHOLD) {
        const reductionRate = AHK_MAX / (AHK_HIGH_THRESHOLD - AHK_LOW_THRESHOLD);
        generalCredit = AHK_MAX - ((incomeForCredits - AHK_LOW_THRESHOLD) * reductionRate);
    }
    
    if (incomeForCredits <= 37000) {
        labourCredit = incomeForCredits * 0.1;
    } else if (incomeForCredits <= 76000) {
        labourCredit = 3700;
    } else {
        labourCredit = 3700 - ((incomeForCredits - 76000) * 0.04);
    }

    return Math.max(0, generalCredit) + Math.max(0, labourCredit);
}

function calculateTax() {
    const income = parseFloat(document.getElementById('income').value) || 0;
    const expenses = parseFloat(document.getElementById('expenses').value) || 0;
    const box2Income = parseFloat(document.getElementById('box2Income').value) || 0;
    const box3Assets = parseFloat(document.getElementById('box3Assets').value) || 0;
    const isEmployee = document.getElementById('isEmployee').checked;
    
    if (income <= 0 && box2Income <= 0 && box3Assets <= 0) {
        alert('Please enter at least one income or asset value.');
        return;
    }
    
    let box1NetProfit = income - expenses;
    let zzpDeductionAmount = 0;
    let totalDeductions = 0;

    if (!isEmployee) {
        const mkbExemption = box1NetProfit * 0.1331;
        box1NetProfit -= mkbExemption;
        totalDeductions += mkbExemption;

        // UPDATED: Zelfstandigenaftrek reduced to ‚Ç¨1,200 for 2026
        const zelfstandigenaftrek = 1200;
        box1NetProfit -= zelfstandigenaftrek;
        totalDeductions += zelfstandigenaftrek;
        zzpDeductionAmount = Math.round(totalDeductions);
    }
    
    const box1TaxableIncome = Math.max(0, box1NetProfit);
    
    let grossBox1Tax = 0;
    
    // UPDATED 2026 BOX 1 RATES: 35.75% / 37.56% / 49.50%
    // UPDATED 2026 BRACKETS: ‚Ç¨38,883 and ‚Ç¨78,426
    if (box1TaxableIncome <= 38883) {
        grossBox1Tax = box1TaxableIncome * 0.3575;
    } else if (box1TaxableIncome <= 78426) {
        grossBox1Tax = (38883 * 0.3575) + ((box1TaxableIncome - 38883) * 0.3756);
    } else {
        grossBox1Tax = (38883 * 0.3575) + (39543 * 0.3756) + ((box1TaxableIncome - 78426) * 0.4950);
    }

    const totalCredits = Math.round(calculateCredits(income, isEmployee));
    let netBox1Tax = Math.max(0, grossBox1Tax - totalCredits);

    let box2Tax = 0;
    if (box2Income > 0) {
        if (box2Income <= 67804) {
            box2Tax = box2Income * 0.245;
        } else {
            box2Tax = (67804 * 0.245) + ((box2Income - 67804) * 0.31);
        }
    }
    
    let box3Tax = 0;
    // UPDATED: Box 3 allowance ‚Ç¨59,357 for 2026
    const box3Allowance = 59357;
    
    if (box3Assets > box3Allowance) {
        const taxableAssets = box3Assets - box3Allowance;
        const deemedReturn = taxableAssets * 0.03;
        box3Tax = deemedReturn * 0.36;
    }
    
    const totalTax = netBox1Tax + box2Tax + box3Tax;
    const totalIncome = income + box2Income;
    const netAfterTax = totalIncome - totalTax;
    const effectiveRate = (totalTax / totalIncome * 100).toFixed(1);
    
    document.getElementById('box1Income').textContent = '‚Ç¨' + income.toLocaleString();
    document.getElementById('box1TaxableBase').textContent = '‚Ç¨' + Math.round(box1TaxableIncome).toLocaleString();
    
    if (!isEmployee && zzpDeductionAmount > 0) {
        document.getElementById('allowanceRow').style.display = 'flex';
        document.getElementById('zzpAllowance').textContent = '-‚Ç¨' + zzpDeductionAmount.toLocaleString();
    } else {
        document.getElementById('allowanceRow').style.display = 'none';
    }
    
    document.getElementById('grossBox1Tax').textContent = '‚Ç¨' + Math.round(grossBox1Tax).toLocaleString();
    document.getElementById('totalCredits').textContent = '-‚Ç¨' + totalCredits.toLocaleString();
    document.getElementById('box1Tax').textContent = '‚Ç¨' + Math.round(netBox1Tax).toLocaleString();
    
    if (box2Income > 0) {
        document.getElementById('box2IncomeDisplay').textContent = '‚Ç¨' + box2Income.toLocaleString();
        document.getElementById('box2Section').style.display = 'block';
        document.getElementById('box2Tax').textContent = '‚Ç¨' + Math.round(box2Tax).toLocaleString();
    } else {
        document.getElementById('box2Section').style.display = 'none';
    }
    
    if (box3Assets > box3Allowance) {
        document.getElementById('box3AssetsDisplay').textContent = '‚Ç¨' + Math.round(box3Assets - box3Allowance).toLocaleString();
        document.getElementById('box3Section').style.display = 'block';
        document.getElementById('box3Tax').textContent = '‚Ç¨' + Math.round(box3Tax).toLocaleString();
    } else {
        document.getElementById('box3Section').style.display = 'none';
    }
    
    document.getElementById('totalTax').textContent = '‚Ç¨' + Math.round(totalTax).toLocaleString();
    document.getElementById('netIncome').textContent = '‚Ç¨' + Math.round(netAfterTax).toLocaleString();
    document.getElementById('effectiveRate').textContent = (totalIncome > 0 ? effectiveRate : 0) + '%';
    
    document.getElementById('results').classList.add('show');
}

