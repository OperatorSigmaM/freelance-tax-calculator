    <script>
        function calculateTax() {
            const income = parseFloat(document.getElementById('income').value) || 0;
            const expenses = parseFloat(document.getElementById('expenses').value) || 0;
            const filingStatus = document.getElementById('filingStatus').value;
            const dependents = parseInt(document.getElementById('dependents').value) || 0;

            const netIncome = income - expenses;
            
            if (netIncome <= 0) {
                 document.getElementById('netIncome').textContent = '$' + Math.max(0, netIncome).toLocaleString('en-US', {maximumFractionDigits: 0});
                 document.getElementById('federalTax').textContent = '$0';
                 document.getElementById('seTax').textContent = '$0';
                 document.getElementById('totalTax').textContent = '$0';
                 document.getElementById('effectiveRate').textContent = '0%';
                 document.getElementById('results').classList.add('show');
                 return;
            }

            // 2025 Standard Deductions (CORRECT)
            const standardDeductions = {
                'S': 15750,
                'J': 31500,
                'H': 23625  // FIXED: was 23575
            };

            // 2025 Federal Income Tax Brackets (CORRECT)
            const federalBrackets = {
                'S': [
                    [0.10, 11925], [0.12, 48475], [0.22, 103350], [0.24, 197300],
                    [0.32, 250525], [0.35, 626350], [0.37, Infinity]
                ],
                'J': [
                    [0.10, 23850], [0.12, 96950], [0.22, 206700], [0.24, 394600],
                    [0.32, 501050], [0.35, 751600], [0.37, Infinity]
                ],
                'H': [
                    [0.10, 17000], [0.12, 64850], [0.22, 103350], [0.24, 197300],
                    [0.32, 250500], [0.35, 626350], [0.37, Infinity]
                ]
            };
            
            // 1. Self-Employment Tax (FICA)
            const selfEmploymentNetEarnings = netIncome * 0.9235;
            let seTax = 0;

            const ssCapGross = 176100;  // FIXED: was 168600, correct 2025 cap 
            const ssCapNetEarnings = ssCapGross * 0.9235; 
            const ssRate = 0.124;
            const medicareRate = 0.029;
            const ahmRate = 0.009;

            seTax += Math.min(selfEmploymentNetEarnings, ssCapNetEarnings) * ssRate;
            seTax += selfEmploymentNetEarnings * medicareRate;

            let ahmThreshold = 0;
            if (filingStatus === 'S' || filingStatus === 'H') ahmThreshold = 200000;
            else if (filingStatus === 'J') ahmThreshold = 250000;

            if (selfEmploymentNetEarnings > ahmThreshold) {
                seTax += (selfEmploymentNetEarnings - ahmThreshold) * ahmRate;
            }
            
            // 2. Calculate Taxable Income
            const halfSETaxDeduction = seTax * 0.5;
            const standardDeduction = standardDeductions[filingStatus];
            
            const AGI = netIncome - halfSETaxDeduction;
            let taxableIncome = Math.max(0, AGI - standardDeduction);

            // 3. Federal Income Tax Calculation
            let federalTax = 0;
            let remaining = taxableIncome;
            let previousLimit = 0;
            
            for (const [rate, limit] of federalBrackets[filingStatus]) {
                if (remaining <= 0) break;
                
                const bracketLimit = limit === Infinity ? remaining + 1 : limit; 
                
                const taxableInBracket = Math.min(remaining, bracketLimit - previousLimit);
                federalTax += taxableInBracket * rate;
                remaining -= taxableInBracket;
                previousLimit = limit;
            }

            // Child Tax Credit with Phase-out
            const maxCTC = 2000;
            let childTaxCredit = dependents * maxCTC;
            
            // CTC Phase-out: starts at $200k (Single/HOH) or $400k (MFJ)
            let phaseOutThreshold = 0;
            if (filingStatus === 'S' || filingStatus === 'H') {
                phaseOutThreshold = 200000;
            } else if (filingStatus === 'J') {
                phaseOutThreshold = 400000;
            }
            
            if (AGI > phaseOutThreshold) {
                // Reduce CTC by $50 for each $1,000 (or fraction thereof) over threshold
                const excessIncome = AGI - phaseOutThreshold;
                const reductionAmount = Math.ceil(excessIncome / 1000) * 50;
                childTaxCredit = Math.max(0, childTaxCredit - reductionAmount);
            }
            
            federalTax = Math.max(0, federalTax - childTaxCredit);

            // 4. Final Totals
            const totalTax = federalTax + seTax;
            const effectiveRate = (netIncome > 0) ? (totalTax / netIncome * 100).toFixed(1) : '0';

            // Display results
            document.getElementById('netIncome').textContent = '$' + netIncome.toLocaleString('en-US', {maximumFractionDigits: 0});
            document.getElementById('federalTax').textContent = '$' + Math.round(federalTax).toLocaleString('en-US', {maximumFractionDigits: 0});
            document.getElementById('seTax').textContent = '$' + Math.round(seTax).toLocaleString('en-US', {maximumFractionDigits: 0});
            document.getElementById('totalTax').textContent = '$' + Math.round(totalTax).toLocaleString('en-US', {maximumFractionDigits: 0});
            document.getElementById('effectiveRate').textContent = effectiveRate + '%';

            document.getElementById('results').classList.add('show');
        }
        
        function addEnterListener(id) {
            document.getElementById(id).addEventListener('keypress', function(e) {
                if (e.key === 'Enter') calculateTax();
            });
        }
        
        addEnterListener('income');
        addEnterListener('expenses');
        addEnterListener('dependents');
    </script>

